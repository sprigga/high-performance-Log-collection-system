  "panels": [
    {
      "id": 1,
      "title": "每秒請求數 (QPS)",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "legendFormat": "總 QPS",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_requests_total{status=~\"2..\"}[1m]))",
          "legendFormat": "成功請求",
          "refId": "B"
        },
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\"}[1m]))",
          "legendFormat": "錯誤請求",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "請求/秒",
          "show": true
        },
        {
          "format": "short",
          "show": false
        }
      ],
      "xaxis": {
        "show": true,
        "mode": "time"
      },
      "lines": true,
      "fill": 0,
      "linewidth": 2,
      "pointradius": 2,
      "points": false,
      "bars": false,
      "stack": false,
      "percentage": false,
      "legend": {
        "show": true,
        "values": true,
        "min": false,
        "max": true,
        "current": true,
        "total": false,
        "avg": true
      },
      "nullPointMode": "connected"
    },
    {
      "id": 2,
